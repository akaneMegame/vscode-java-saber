{
    "name": "vscode-java-saber",
    "displayName": "vscode-java-saber",
    "description": "Some useful functions to help coding.",
    "version": "0.0.1",
    "publisher": "bo1318",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.newAnnotationFile",
        "onCommand:extension.newClassFile",
        "onCommand:extension.newEnumFile",
        "onCommand:extension.newInterfaceFile",
        "onCommand:extension.generateGettersAndSetters"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.newAnnotationFile",
                "title": "Java: New Annotation File"
            },
            {
                "command": "extension.newClassFile",
                "title": "Java: New Class File"
            },
            {
                "command": "extension.newEnumFile",
                "title": "Java: New Enum File"
            },
            {
                "command": "extension.newInterfaceFile",
                "title": "Java: New Interface File"
            },
            {
                "command": "extension.generateGettersAndSetters",
                "title": "Java: Generate Getters and Setters..."
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
